{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/signin", "dest": "/signin.html" },
    { "src": "/signup", "dest": "/signup.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}